<< chunk_load, eval=TRUE,echo=TRUE>>=
library("missingHE")
@
\vspace{5pt}
\item MenSS data already available in the object \texttt{MenSS} when loading the package
<< chunk_data, eval=TRUE,echo=TRUE>>=
str(MenSS)
@
\end{itemize}
