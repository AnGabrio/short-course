\begin{itemize}
\item If $p<0.01 \Rightarrow$ \textbf{strong} evidence against $H_0$
\item If $0.01<p<0.05 \Rightarrow$ \textbf{fairly strong} evidence against $H_0$
\item If $p>0.05 \Rightarrow$ \textbf{little or no} evidence against $H_0$
\end{itemize}
};
\draw(4,1) node{\includegraphics[scale=.2]{figure/peevalue.jpg}};
