\begin{frame}<handout:0>[noframenumbering]
\frametitle{Problems \textbf{with HTA} data -- Skewness}

<< chunk_skewness, eval=TRUE,echo=FALSE,comment=NA,warning=FALSE,error=FALSE,message=FALSE,fig.width=7,fig.height=7,out.width='50%',fig.align='center'>>=
library(fitdistrplus)
n<-50
mu<-500
sigma<-500
shape<-mu^2/(sigma^2)
rate<-mu/(sigma^2)
set.seed(12345)
outcome<-rgamma(n=n,shape = shape, rate = rate)
fit1.gamma <- fitdist(outcome, distr = c("gamma"), method = "mme")
fit2.lnorm <- fitdist(outcome, distr = c("lnorm"), method = "mme")
fit3.norm <- fitdist(outcome, distr = c("norm"), method = "mme")
plot.legend <- c("Gamma", "LogNormal","Normal")
denscomp(list(fit1.gamma,fit2.lnorm,fit3.norm),legendtext = plot.legend,ylim = c(0,0.002),xlim = c(-500,2000),
         xlab = "Costs", fitlty=c(1,1,1), fitcol = c("blue","darkgreen","red"), lwd=c(2,2,2), cex=1,
         main = "")
@
\begin{itemize}
\item Alternative parametric distributions have better fit to skewed data (e.g. \textbf{\blue Gamma} or \textbf{\olive LogNormal})
\item When the sample size is not large, inferences based on the \textbf{\red Normal} distribution may be incorrect
\end{itemize}
\end{frame}


\UCL 
\begin{frame}<handout:0>[noframenumbering]
\frametitle{Problems \textbf{with HTA} data -- Spikes}

<< chunk_ones, eval=TRUE,echo=FALSE,comment=NA,warning=FALSE,error=FALSE,message=FALSE,fig.width=7,fig.height=7,out.width='50%',fig.align='center'>>=
library(fitdistrplus)
n<-100
mu<-0.8
sigma<-0.08
shape1<-mu*((mu*(1-mu)/sigma^2)-1)
shape2<-(1-mu)*((mu*(1-mu)/sigma^2)-1)
set.seed(12345)
outcome<-rbeta(n=n,shape1 = shape1,shape2 = shape2)
outcome<-c(outcome,rep(1,30))
fit1.beta <- fitdist(outcome, distr = c("beta"), method = "mme")
fit3.norm <- fitdist(outcome, distr = c("norm"), method = "mme")
plot.legend <- c("Beta","Normal")
denscomp(list(fit1.beta,fit3.norm),legendtext = plot.legend,ylim = c(0,6),xlim = c(0.5,1.1),
         xlab = "Utilities", fitlty=c(1,1), fitcol = c("blue","red"), lwd=c(2,2), cex=1,
         main = "")

@
\begin{itemize}
\item Spikes often induce high skewness which may be difficult to capture using standard distributions
\item The \textbf{\blue Beta} distribution has a better fit than the \textbf{\red Normal} but is not defined at 1 
\end{itemize}
\end{frame}


